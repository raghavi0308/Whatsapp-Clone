{
    "builds": [
      { "src": "package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      {
        "src": "/manifest.json",
        "dest": "/manifest.json",
        "methods": ["GET"],
        "headers": { "cache-control": "public, max-age=31536000, immutable" }
      },
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  